[autoload]

BaseData="*res://Base/GlobalScripts/BaseData.gd"
InputSystem="*res://Base/GlobalScripts/InputSystem.gd"
CommonUI="*res://Base/CommonUI/CommonUI.tscn"
Utils="*res://Game/GlobalScripts/Utils.gd"
GameEnums="*res://Game/GlobalScripts/GameEnums.gd"
GameData="*res://Game/GlobalScripts/GameData.gd"
GameFuncs="*res://Game/GlobalScripts/GameFuncs.gd"

[display]

window/size/width=640
window/size/height=640
window/size/resizable=false

[input]

ui_quit={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777217,"unicode":0,"echo":false,"script":null)
 ]
}

